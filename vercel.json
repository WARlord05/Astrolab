{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_API_URL_SECONDARY": "@vite_api_url_secondary",
    "VITE_API_URL_TERTIARY": "@vite_api_url_tertiary"
  }
}
